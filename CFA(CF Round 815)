#include <bits/stdc++.h>
using namespace std;
#define ll long long int

int main() {
	// your code goes here
	ll tc;
	cin>>tc;
	for(ll i=0;i<tc;i++){
	   ll n,m,x,y;
	    cin>>n>>m>>x>>y;
	    if(n*y==m*x){
	        cout<<0<<endl;
	    }
	    else if(((n*y)!=0 && (m*x)%(n*y)==0) || ((m*x)!=0 && (n*y)%(m*x)==0)){
	        cout<<1<<endl;
	    } 
	    else{
	        cout<<2<<endl;
	    }
	}
	return 0;
}
