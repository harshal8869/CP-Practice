struct cmp{
	bool operator()(pair<int,int> a,pair<int,int> b){
		if((a.second-a.first)>(b.second-b.first)){
			return 0;
		}
		else if((a.second-a.first)==(b.second-b.first)){
			if(a.first<b.first){
				return 0;
			}
			else{
				return 1;
			}
		}
		else{
			return 1;
		}
	}
};
 
 
 ll formulae(ll l, ll last){
    return (last * (last + 1)) / 2 - (l * (l - 1)) / 2;
}
 
 
void thrill(){    
 ll a,b,c; cin>>a>>b>>c;

  ll d = c/2;
  ll sum1 = a + d + (c%2);
  ll sum2 = b + d;
  if(sum1 > sum2){
      cout<<"First"<<endl;
  }
else{
    cout<<"Second"<<endl;
}
 
  
}
    int main(){  
    int tc;
     cin>>tc;
    for(int i=0;i<tc;i++){
            thrill();
        }
        return 0;
    }
        
    
