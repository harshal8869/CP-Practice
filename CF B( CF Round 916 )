#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main(){
    int tc;
    cin>>tc;
    for(int i=0;i<tc;i++){
       int n,m;
       cin>>n>>m;
        for(int i=n-m;i<n+1;i++){
            cout<<i<<" ";
        }
        
        for(int i=n-m-1;i>0;i--){
          cout<<i<<" ";
        }
        
        cout<<endl;
    }
    return 0;
}
