#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	      int n;
        cin>>n;
        int cnt=0;
        for(int i=0;n!=50;i++){
             if(n==50 || n < 50){
              n+=2;
              cnt++;
             }
             else{
             n-=3;
                 cnt++;
             }
             
             
        }
        cout<<cnt<<endl;
	}
	return 0;
}
