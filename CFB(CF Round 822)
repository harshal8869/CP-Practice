#include<bits/stdc++.h>
using namespace std;
void pyramid(){
    int n;
    cin>>n;
for (int i=1; i<=n; i++) {
        for (int j=1;j<=i;j++) {
 
            if (j==1 || j==i){
            cout<<1<<" ";
}
            else{
            cout<<0<<" ";
}
        }
        cout<<endl;

    }


}
int main(){
    int tc;
    cin>>tc;
    for(int i=0;i<tc;i++){
        pyramid();
    }
    return 0;
}
