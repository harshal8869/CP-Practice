#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int tc;
	cin>>tc;
	for(int i=0;i<tc;i++){
	    int p,q,r,s;
	    cin>>p>>q>>r>>s;
	    if((p > q + r + s) || (q > r + s + p) || (r > p + q + s) || (s > p + q + r)){
	        cout<<"yes"<<endl;
	    }
	    else{
	        cout<<"no"<<endl;
	    }
	}
	return 0;
}
