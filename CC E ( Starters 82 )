#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int tc;
	cin>>tc;
	for(int i=0;i<tc;i++){
	    int n;
	    cin>>n;
	    int a = 3;
	    int b = 2;
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            if(n*n >= a){
                cout<<a<<" ";
                a+=2;
            }
            else{
                cout<<b<<" ";
                b+=2;
            }
        }
        cout<<endl;
    }
	}
	return 0;
}
