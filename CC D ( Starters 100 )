#include <bits/stdc++.h>
using namespace std;

void thrill(){
    int n;
    cin>>n;
    string s;
    cin>>s;
    bool chk = false;
    for(int i=0;i<n;i++){
         if(chk==1 || s[i]=='0'){
	   s[i]='0';
	   }
	   else if(n==i+2){
	       break;
	   }
	   else{
	       s[i]='1';
	       chk=true;
	   }
    }
    cout<<s<<endl;
}

int main(){
    int tc;
    cin>>tc;
    for(int i=0;i<tc;i++){
        thrill();
    }
    return 0;
}
