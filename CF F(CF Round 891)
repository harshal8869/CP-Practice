struct cmp{
	bool operator()(pair<int,int> a,pair<int,int> b){
		if((a.second-a.first)>(b.second-b.first)){
			return 0;
		}
		else if((a.second-a.first)==(b.second-b.first)){
			if(a.first<b.first){
				return 0;
			}
			else{
				return 1;
			}
		}
		else{
			return 1;
		}
	}
};
 
 
 
 
 
void thrill(){
     ll n;
    cin>>n;
    map<ll,ll>mp;
    for(int i=0;i<n;i++){
        ll x;
        cin>>x;
        mp[x]++;
    }
    ll q;
    cin>>q;
    while(q--){
        ll x,y;
        cin>>x>>y;
        if(x*x-4*y<0){
            cout<<0<<" ";
            continue;
        }
        ll sq=(sqrt(x*x-4*y));
        ll sq2=(x+sq)/2;
        ll sq3=(x-sq)/2;
        if(sq2 + sq3 == x && sq2 * sq3 == y && mp.count(sq2) && mp.count(sq3)){
            if(sq2 == sq3){
                cout<<mp[sq2] * (mp[sq2]-1)/2<<" ";
            }
            else{
                cout<<mp[sq2] * mp[sq3]<<" ";
            }
        }
        else{
            cout<<0<<" ";
        }
    }
  cout<<endl;
}
    int main(){
     int tc;
      cin>>tc;
      for(int i=0;i<tc;i++){
            thrill();
        }
        return 0;
    }
        
    
