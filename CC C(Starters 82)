#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int tc;
	cin>>tc;
	for(int i=0;i<tc;i++){
	    int n;
	    cin>>n;
	    int cnt=0;
	    int arr[n];
	    for(int i=0;i<n;i++){
	        cin>>arr[i];
	    }
	    sort(arr,arr+n);
	    for(int i=0;i<n;i++){
	        if(arr[i] > arr[0])
	        cnt++;
	    }
	    cout<<cnt<<endl;
	}
	return 0;
}
