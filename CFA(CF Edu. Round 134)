#include<bits/stdc++.h>
using namespace std;
int main(){
    int tc;
    cin>>tc;
    for(int i=0;i<tc;i++){
        char ch;
        int n=4;
        set<char>st;
        while(n--){
            cin>>ch;
            st.insert(ch);
        }
        int m=st.size();
        cout<<m-1<<endl;
 
    }
    return 0;
}
