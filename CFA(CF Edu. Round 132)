#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int x;
	    cin>>x;
	    int n=3;
	    int arr[n];
	    for(int i=0;i<n;i++){
	        cin>>arr[i];
	        
	    }
	    if(arr[x-1]==0){
	        cout<<"NO"<<endl;
	        continue;
	       
	    }
	    if(arr[arr[x-1]-1]==0){
	        cout<<"NO"<<endl;
	        continue;
	        
	    }
	    else{
	        cout<<"YES"<<endl;
	        continue;
	    }
	}
	
}
