#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int tc;
	cin>>tc;
	for(int i=0;i<tc;i++){
	    int a,b,c,d;
	    cin>>a>>b>>c>>d;
	    set<int>st;
	    for(int i=a;i<=b;i++){
	        st.insert(i);
	    }
	    for(int i=c;i<=d;i++){
	        st.insert(i);
	    }
	    int ans = st.size();
	    cout<<ans<<endl;
	    
	}
	return 0;
}
