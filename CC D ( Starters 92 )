#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int tc;
	cin>>tc;
	for(int i=0;i<tc;i++){
	    int a,b;
	    cin>>a>>b;
	    
	   int pr = a * b;
	     if(a%2==1 && b%2==1){
	        cout<<"NO"<<endl;
	    }
	    
	    else if(pr & 1){
	        cout<<"NO"<<endl;
	    }
	    else{
	        cout<<"YES"<<endl;
	    }
	}
	return 0;
}
