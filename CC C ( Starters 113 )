#include<bits/stdc++.h>
using namespace std;
#define ll long long int
void thrill(){
    int n;
    cin>>n;
    int cnt=0;
    unordered_map<int,int>mp;
    vector<int>v(n);
    for(int i=0;i<n;i++){
        cin>>v[i];
    }
     int a = count(v.begin(),v.end(),1);
     int b = n - a;
     int ans = min(a,b);
     cout<<ans<<endl;
}
int main(){
    int tc;
    cin>>tc;
    for(int i=0;i<tc;i++){
        thrill();
    }
}
