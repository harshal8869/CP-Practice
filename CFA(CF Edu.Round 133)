#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        int n;
        cin>>n;
        if(n==1)
        {
            cout<<2<<endl;
            
        }
        else{
            
        
        bool flag=n%3!=0;
        cout<<flag + n/3<<endl;
        }
    }
    return 0;
}



