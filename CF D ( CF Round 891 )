struct cmp{
	bool operator()(pair<int,int> a,pair<int,int> b){
		if((a.second-a.first)>(b.second-b.first)){
			return 0;
		}
		else if((a.second-a.first)==(b.second-b.first)){
			if(a.first<b.first){
				return 0;
			}
			else{
				return 1;
			}
		}
		else{
			return 1;
		}
	}
};
 
 
 
 
 
void thrill(){
    
    ll n;
    cin>>n;
    ll arr[n];
    for(int i=0;i<n;i++){
         cin>>arr[i];
        }
        ll brr[n];
        for(int i=0;i<n;i++){
         cin>>brr[i];
         }
          ll mini = INT_MAX;
        for(int i=0;i<n;i++){
            arr[i] = brr[i] - arr[i];
         mini = min(arr[i],mini);
        }
        vector<int>vec;
        for(int i=0;i<n;i++){
          if(mini==arr[i]){
          vec.push_back(i+1);
          }
        }
        ll len = vec.size();
        cout<<len<<endl;
       for(int i=0;i<len;i++){
           cout<<vec[i]<<" ";
       }
       cout<<endl;
}
    int main(){
     int tc;
      cin>>tc;
      for(int i=0;i<tc;i++){
            thrill();
        }
        return 0;
    }
        
    
