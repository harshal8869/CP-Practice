#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int tc;
	cin>>tc;
	for(int i=0;i<tc;i++){
	    int n;
	    cin>>n;
	    int arr[n];
	    for(int i=0;i<n;i++){
	        cin>>arr[i];
	    }
	    int cnt=0;
	    int cnt1=0;
	    for(int i=0;i<n;i++){
	        if(arr[i]==0)
	            cnt++;
	  
	    }
	    if(cnt%2!=0){
	    cout<<"no"<<endl;
	    }
	    else {cout<<"yes"<<endl;}
	   
	}
	return 0;
}
