#include<bits/stdc++.h>
using namespace std;

void thrill(){
    int n,m;
    cin>>n>>m;
    string s;
    cin>>s;
    if(s[0]=='0'){
        m--;
        s[0]='1';
    }
    while(m--) 
    s+='0';
    cout<<s<<endl;
}

int main() {
 int tc;
 cin>>tc;
 for(int i=0;i<tc;i++){
     
  thrill();
 }
 return 0;
}
