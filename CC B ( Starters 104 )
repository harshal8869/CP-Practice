#include<bits/stdc++.h>
using namespace std;
void thrill(){
    int n;
    cin>>n;
    if(n==1){
        cout<<"YES"<<endl;
    }
    else if(n%2==0){
        cout<<"YES"<<endl;
    }
    else{
        cout<<"NO"<<endl;
    }
}
int main(){
    int tc;
    cin>>tc;
    for(int i=0;i<tc;i++){
        thrill();
    }
}
