#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main(){
    int tc;
    cin>>tc;
    for(int i=0;i<tc;i++){
     int n;
    cin>>n;
    int x,y;
    int idx=1;
    int ans=0;
    for(int i=0;i<n;i++){
        cin>>x>>y;
        if(x<=10 && y>ans){
            ans =y;
            idx=i+1;
        }
    }
    cout<<idx<<endl;
    }
    return 0;
}
