#include <bits/stdc++.h>
using namespace std;
#define ll long long int
void thrill(){
    ll a,b;
    cin>>a>>b;
    if(4*b>=3*a){
        cout<<"YES"<<endl;
    }
    else{
        cout<<"NO"<<endl;
    }
}
int main() {
	// your code goes here
	int tc;
	cin>>tc;
	for(int i=0;i<tc;i++){
	    thrill();
	}

}
